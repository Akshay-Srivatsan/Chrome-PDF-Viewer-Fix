{
  "name": "Menlo School Haiku PDF Viewer",
  "manifest_version": 2,
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery/jquery.js", "main.js", "humane-js/humane.min.js"],
      "css": ["humane-js/libnotify.css"]
    }],
  "permissions": [
    "https://haiku.menloschool.org/*"
  ],
  "author": "Akshay Srivatsan",
  "description": "Fixes a bug with Haiku Learning and Chrome's PDF Viewer.",
  "homepage_url": "https://github.com/Akshay-Srivatsan/Chrome-PDF-Viewer-Fix",
  "update_url": "https://raw.githubusercontent.com/Akshay-Srivatsan/Chrome-PDF-Viewer-Fix/master/bin/update.xml"
}
